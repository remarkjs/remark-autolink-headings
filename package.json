{
  "name": "remark-autolink-headings",
  "version": "4.0.0",
  "description": "Automatically add links to headings in Markdown.",
  "main": "dist/index.js",
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "pretest": "eslint src",
    "prepublish": "del-cli dist && babel src --out-dir dist --ignore /__tests__/",
    "report": "nyc report --reporter=html",
    "test": "nyc ava",
    "test-012": "nyc ava"
  },
  "keywords": [
    "headings",
    "link",
    "automatic",
    "markdown",
    "remark"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.0.0",
    "eslint-config-cssnano": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "remark": "^6.0.0",
    "remark-html": "^5.0.0",
    "remark-slug": "^4.1.0",
    "nyc": "^10.0.0"
  },
  "homepage": "https://github.com/ben-eb/remark-autolink-headings",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/remark-autolink-headings",
  "dependencies": {
    "unist-util-visit": "^1.0.1"
  },
  "eslintConfig": {
    "extends": "cssnano"
  },
  "ava": {
    "require": "babel-register"
  }
